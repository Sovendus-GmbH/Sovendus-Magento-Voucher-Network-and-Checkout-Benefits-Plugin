parameters:
  level: 5
  treatPhpDocTypesAsCertain: true
  paths:
    - .
  bootstrapFiles:
    - %currentWorkingDirectory%/vendor/autoload.php
  
  # Load Magento stubs from the installed package.
  scanFiles:
    - %currentWorkingDirectory%/vendor/magento/magento2-functional-testing-framework/src/Magento/FunctionalTestingFramework/_files/Magento.php

  excludePaths:
    - vendor/*