{
  "name": "sovendus/module-sovendus",
  "description": "Sovendus App for Magento",
  "keywords": [
    "Sovendus",
    "Sovendus Voucher Network",
    "Sovendus CHeckout Benefits",
    "magento",
    "magento 2",
    "magento order thank you",
    "magento order success",
    "magento 2 order thank you page",
    "magento 2 order success page"
  ],
  "type": "magento2-module",
  "version": "2.0.0-beta-3",
  "require": {
    "magento/framework": ">=102.0.0",
    "magento/module-store": ">=101.0.0",
    "magento/module-sales": ">=102.0.0",
    "magento/module-checkout": ">=100.0.0",
    "magento/module-config": ">=101.0.0"
  },
  "license": [
    "GPL-3.0"
  ],
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Sovendus\\SovendusApp\\": ""
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "require-dev": {
    "magento/magento2-functional-testing-framework": "dev-master",
    "phpstan/phpstan": "^1.0",
    "squizlabs/php_codesniffer": "^3.0",
    "phpcompatibility/php-compatibility": "^9.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "magento/composer-dependency-version-audit-plugin": true
    }
  }
}